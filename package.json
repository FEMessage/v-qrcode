{
  "name": "@femessage/v-qrcode",
  "version": "1.0.0",
  "description": "",
  "author": "https://github.com/FEMessage",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/FEMessage/v-qrcode.git"
  },
  "keywords": [
    "vue",
    "sfc",
    "component"
  ],
  "main": "dist/v-qrcode.umd.js",
  "module": "dist/v-qrcode.esm.js",
  "unpkg": "dist/v-qrcode.min.js",
  "browser": {
    "./sfc": "src/v-qrcode.vue"
  },
  "scripts": {
    "styleguide": "vue-styleguidist server",
    "story": "start-storybook -p 9001 -c .storybook",
    "story:build": "build-storybook -c .storybook -o docs/storybook",
    "doc": "vue-styleguidist build",
    "test": "jest",
    "build": "npm run build:unpkg & npm run build:es & npm run build:umd & npm run doc & npm run story:build",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/v-qrcode.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/v-qrcode.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/v-qrcode.min.js",
    "prepublishOnly": "npm run build",
    "precommit": "pretty-quick --staged",
    "postpublish": "git add docs && git add dist && git commit -a --amend"
  },
  "dependencies": {
    "qr.js": "^0.0.0"
  },
  "devDependencies": {
    "@storybook/vue": "^3.4.10",
    "@vue/test-utils": "^1.0.0-beta.16",
    "babel-jest": "^23.0.1",
    "babel-preset-env": "^1.7.0",
    "husky": "^0.14.3",
    "jest": "^23.1.0",
    "minimist": "^1.2.0",
    "prettier": "1.12.1",
    "pretty-quick": "^1.4.1",
    "rollup": "^0.57.1",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-uglify-es": "0.0.1",
    "rollup-plugin-vue": "^3.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue": "^2.5.16",
    "vue-jest": "^2.6.0",
    "vue-styleguidist": "1.7.7",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^3.10.0"
  },
  "files": [
    "src",
    "dist",
    "yarn.lock"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "./node_modules/babel-jest",
      "^.+\\.vue$": "./node_modules/vue-jest"
    }
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  }
}
